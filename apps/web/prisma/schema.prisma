datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider        = "prisma-client-js"
}

model Space {
  id            String    @id @default(cuid())
  name          String   
  title         String  
  description   String
  image         String
  type          String
  isStarRating  Boolean
}

model Feedbacks {
  id            String    @id @default(cuid())
  feedback      String   
  name          String  
  email         String
  image         String
  rating        Int
  spaceId       String
}